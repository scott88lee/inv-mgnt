<div>
	<h1>Product List</h1>
	<div class="ui cards">
		{{#each product}}
			<div class="card">
				<div class="content">
					<div class="header">{{model}}</div>
					<div class="meta">
						<a href='/products/{{sku}}'>
						{{sku}}</a>
					</div>
					<div class="description">
						{{description}}
					</div>
					{{this.model}}<br>
					{{this.supplier}}<br>
					{{this.color}}<br>
					{{this.variation}}<br>
				</div>
			</div>
		{{/each}}
	</div>
</div>
